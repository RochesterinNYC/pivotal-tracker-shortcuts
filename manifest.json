{
  "manifest_version": 2,
  "name": "Pivotal Tracker Shortcuts",
  "description": "This extension will provide easier, sole navigation of Pivotal Tracker through the keyboard.",
  "version": "0.0.1",
  "permissions": [
    "http://www.pivotaltracker.com/*",
    "https://www.pivotaltracker.com/*"
  ],
  "content_scripts": [{
    "matches": [
      "http://www.pivotaltracker.com/*",
      "https://www.pivotaltracker.com/*"
    ],
    "css": ["src/css/pivotal-tracker-shortcuts.css"],
    "js": [
      "src/js/jquery-2.1.4.min.js",
      "src/js/pivotal-tracker-shortcuts.js"
    ]
  }]
}
